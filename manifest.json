{
	"manifest_version": 2,
	"name": "The PR Formatter",
	"description": "The PR Formatter",
	"version": "1.0.0",
	"icons": {"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "Formatter.html"
	},
    "permissions": ["tabs"],
    "web_accessible_resources": ["Formatter.js"]
}